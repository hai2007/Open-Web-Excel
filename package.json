    "@babel/preset-env": "^7.0.0",
    "@hai2007/nodejs": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "rollup": "^1.17.0",
    "uglify-js": "^3.5.6",
